{
  "name": "electron-vue-ts-template",
  "private": true,
  "version": "0.0.1",
  "description": "A minimal Electron + Vue + Typescript application",
  "main": "build/main/main.js",
  "author": "bonehead55",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "dev-srv": "vite",
    "dev-app": "electron ./build/main/main.js 3000",
    "check-srv": "vue-tsc --noEmit --project tsconfig.vite.json",
    "build": "vue-tsc --noEmit --project tsconfig.vite.json && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^17.0.17",
    "@vitejs/plugin-vue": "^2.2.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "copyfiles": "^2.4.1",
    "electron": "^16.0.8",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  }
}
